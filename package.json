{
  "name": "tiktok-stream-tool",
  "version": "1.0.3",
  "description": "Professional TikTok LIVE streaming tool with overlays, alerts, TTS and automation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:css": "tailwindcss -i ./public/css/tailwind.input.css -o ./public/css/tailwind.output.css --minify",
    "watch:css": "tailwindcss -i ./public/css/tailwind.input.css -o ./public/css/tailwind.output.css --watch",
    "build": "npm run build:css"
  },
  "keywords": [
    "tiktok",
    "streaming",
    "obs",
    "overlay",
    "alerts",
    "tts"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0 <24.0.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.6",
    "axios": "^1.6.5",
    "better-sqlite3": "^11.9.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "franc-min": "^6.2.0",
    "matter-js": "^0.20.0",
    "multer": "^2.0.2",
    "obs-websocket-js": "^5.0.6",
    "open": "^10.0.3",
    "osc": "^2.4.5",
    "socket.io": "^4.6.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "systeminformation": "^5.27.11",
    "tiktok-live-connector": "^2.1.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3",
    "zip-lib": "^1.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  }
}

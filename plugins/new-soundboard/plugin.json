{
  "id": "new-soundboard",
  "name": "New Soundboard",
  "version": "1.0.0",
  "description": "Modern, security-hardened soundboard with MyInstants API integration and iframe fallback support",
  "author": "Pup Cid",
  "entry": "main.js",
  "enabled": true,
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.18.3",
    "multer": "^2.0.2",
    "express-rate-limit": "^8.2.1",
    "myinstants-api": "^1.0.0",
    "p-queue": "^8.0.1",
    "node-fetch": "^2.7.0"
  },
  "routes": [
    "/api/new-soundboard/*",
    "/new-soundboard/*"
  ],
  "permissions": [
    "filesystem.read",
    "filesystem.write",
    "network.http",
    "websocket"
  ]
}
